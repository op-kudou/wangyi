.musiclist-container{
    display: flex;
    padding: 14rpx 20rpx;
    align-self: center;
}
.musiclist-index{
    color: #888888;
    font-size: 34rpx;
    width: 80rpx;
}
.musiclist-info{
    flex-grow: 1;
    width: 0;
}
.musiclist-name{
    font-size: 34rpx;
    color: #333333;
    overflow: hidden;
    text-overflow: ellipsis;
}
.musiclist-singer{
  font-size: 24rpx;
  color:#888;
}
.playing{
  color:#0099ff !important;
}